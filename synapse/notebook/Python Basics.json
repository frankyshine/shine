						}
					}
				},
				"source": [
					"# note that the table name is different from the location folder\r\n",
					"spark.sql(''' CREATE TABLE cities1 (name STRING, population INT) \r\n",
					"              USING PARQUET  \r\n",
					"              LOCATION '/datalake/cities' OPTIONS ('compression'='snappy')''')"
				],
				"execution_count": 7
